{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@images/*": ["public/images/*"],
            "@src/*": ["src/*"],
            "@assets/*": ["src/assets/*"],
            "@components/*": ["src/components/*"],
            "@configs/*": ["src/config/*"],
            "@contexts/*": ["src/contexts/*"],
            "@hooks/*": ["src/hooks/*"],
            "@pages/*": ["src/pages/*"],
            "@routes/*": ["src/routes/*"],
            "@services/*": ["src/services/*"],
            "@utils/*": ["src/utils/*"]
        }
    },
    "include": ["src"]
}
